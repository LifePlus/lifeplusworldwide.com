---
import Container from '../Container.vue'
import Card from '../Card.astro'
const { content } = Astro.props
---

<div class="py-12">
  <Container>
    <h2 class="text-sm font-medium pb-8">{content.title}</h2>
    <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
      {content.cards.map(content => <Card content={content} />)}
    </div>
  </Container>
</div>
